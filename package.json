{
  "name": "revtaku",
  "version": "1.0.0",
  "description": "Review App",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "14.9.0"
  },
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"nodemon backend/server.js\" \"npm run client\" ",
    "data:import": "node backend/seeder",
    "data:delete": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manRexx/revTaku.git"
  },
  "author": "Manik Rawat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manRexx/revTaku/issues"
  },
  "homepage": "https://github.com/manRexx/revTaku#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "googleapis": "^39.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
